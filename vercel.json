{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/seguimiento", "destination": "/seguimiento.html" },
    { "source": "/((?!api/|seguimiento).*)", "destination": "/index.html" }
  ]
}
